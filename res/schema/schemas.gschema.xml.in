<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="@gettext@">
  <schema id="@rdnn@" path="@path@">
    <key type="b" name="minimize">
        <default>false</default>
        <summary>minimize to panel</summary>
    </key>
    <key type="b" name="draggable">
        <default>false</default>
        <summary>make lyric draggable or not</summary>
    </key>
    <key type="b" name="online-lyrics">
        <default>true</default>
        <summary>auto download missing lyrics</summary>
    </key>
    <key type="u" name="online-provider">
        <range min="0" max="2"/>
        <default>0</default>
        <summary>online lyric provider (0 - ncm, 1 - ncm (trans), 2 - lrclib)</summary>
    </key>
    <key type="b" name="online-fallback">
        <default>false</default>
        <summary>fallback the 1st result if no match</summary>
    </key>
    <key type="b" name="show-progress">
        <default>true</default>
        <summary>show lyric playing progress</summary>
    </key>
    <key type="u" name="systray-area">
        <default>0</default>
        <summary>systray status area (0 - left, 1 - center, 2 - right)</summary>
    </key>
    <key type="u" name="lyric-orientation">
        <range min="0" max="1"/>
        <default>0</default>
        <summary>lyric orientation (0 - horizontal, 1 - vertical)</summary>
    </key>
    <key type="u" name="refresh-interval">
        <range min="20" max="500"/>
        <default>60</default>
        <summary>lyric refresh interval (ms)</summary>
    </key>
    <key type="s" name="lyric-location">
        <default>""</default>
        <summary>location to save and read lyrics</summary>
    </key>
    <key type="u" name="lyric-opacity">
        <range min="20" max="100"/>
        <default>50</default>
        <summary>opacity percent of desktop lyric</summary>
    </key>
    <key type="s" name="font-name">
        <default>"Sans 32"</default>
        <summary>font name of desktop lyric</summary>
    </key>
    <key type="(uu)" name="lyric-place">
        <default>(200, 200)</default>
        <summary>desktop lyric place</summary>
    </key>
    <key type="b" name="allow-video-players">
        <default>true</default>
        <summary>allow video players with MPRIS support</summary>
    </key>
    <key type="s" name="preferred-mpris-player">
        <default>""</default>
        <summary>preferred MPRIS player (empty for auto)</summary>
    </key>
    <key type="u" name="panel-width">
        <range min="100" max="800"/>
        <default>300</default>
        <summary>panel lyric fixed width (pixels)</summary>
    </key>
  </schema>
</schemalist>
